{
  "name": "slidev-theme-gyori-cogex",
  "version": "0.1.0",
  "description": "Slidev theme for INDRA CoGEx MCP analysis - computational biology presentations with Gyori Lab aesthetic",
  "keywords": [
    "slidev-theme",
    "slidev",
    "indra",
    "cogex",
    "computational-biology",
    "gyori-lab"
  ],
  "author": "Gyori Lab",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.50.0"
  },
  "slidev": {
    "colorSchema": "dark",
    "defaults": {
      "aspectRatio": "16/9",
      "canvasWidth": 980,
      "fonts": {
        "sans": "Inter",
        "mono": "JetBrains Mono"
      },
      "highlighter": "shiki",
      "themeConfig": {
        "primary": "#17a2b8",
        "secondary": "#BF40BF",
        "success": "#5bb900",
        "warning": "#ffc107",
        "danger": "#dc3545",
        "neutral": "#6c757d"
      }
    }
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.2.0",
    "@iconify-json/mdi": "^1.2.0",
    "@iconify-json/ph": "^1.2.0",
    "@unocss/reset": "^0.60.0"
  },
  "devDependencies": {
    "@slidev/cli": "^52.11.3",
    "@slidev/theme-default": "^0.25.0",
    "@slidev/types": "^0.50.0"
  }
}
